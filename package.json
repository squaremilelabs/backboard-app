{
  "name": "backboard-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "npx zenstack generate",
    "db:push": "npx prisma db push --skip-generate",
    "db:reset": "npx prisma db push --skip-generate --force-reset && npx prisma db seed"
  },
  "prisma": {
    "schema": "src/database/generated/prisma-schema/schema.prisma",
    "seed": "ts-node src/database/seed.ts"
  },
  "zenstack": {
    "schema": "src/database/schema/schema.zmodel"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.11.2",
    "@prisma/client": "6.3.x",
    "@szhsin/react-accordion": "^1.4.0",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-query-devtools": "^5.66.0",
    "@zenstackhq/runtime": "2.11.6",
    "@zenstackhq/server": "2.11.6",
    "formik": "^2.4.6",
    "lucide-react": "^0.474.0",
    "next": "15.1.6",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-aria-components": "^1.6.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.1",
    "tailwind-variants": "^0.3.1",
    "zod": "^3.24.1",
    "zod-formik-adapter": "^1.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@zenstackhq/tanstack-query": "2.11.6",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "postcss": "^8",
    "prettier-plugin-classnames": "^0.7.6",
    "prettier-plugin-merge": "^0.7.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "6.3.x",
    "tailwindcss": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "zenstack": "2.11.6"
  }
}
