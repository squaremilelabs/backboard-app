{
  "name": "backboard-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "vercel-build": "pnpm vercel-build-no-db",
    "vercel-build-no-db": "pnpm db:generate && pnpm build",
    "vercel-build-with-db": "pnpm db:generate && pnpm db:migrate-deploy && pnpm build",
    "db:generate": "npx zenstack generate",
    "db:migrate-deploy": "npx prisma migrate deploy",
    "db:dev:push": "npx prisma db push --skip-generate",
    "db:dev:reset": "npx prisma db push --skip-generate --force-reset",
    "db:dev:migrate-dev": "npx prisma migrate dev --skip-generate",
    "db:dev:migrate-reset": "npx prisma migrate reset --skip-generate"
  },
  "prisma": {
    "schema": "src/database/generated/prisma-schema/schema.prisma"
  },
  "zenstack": {
    "schema": "src/database/schema/schema.zmodel"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.11.2",
    "@mdi/js": "^7.4.47",
    "@mdi/react": "^1.6.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "6.3.x",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-query-devtools": "^5.66.0",
    "@vercel/analytics": "^1.5.0",
    "@zenstackhq/runtime": "2.11.6",
    "@zenstackhq/server": "2.11.6",
    "date-fns": "^4.1.0",
    "emoji-picker-react": "^4.12.2",
    "formik": "^2.4.6",
    "lucide-react": "^0.474.0",
    "next": "15.2.4",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-aria": "^3.37.0",
    "react-aria-components": "^1.6.0",
    "react-dom": "^19.0.0",
    "react-stately": "^3.36.1",
    "react-textarea-autosize": "^8.5.7",
    "svix": "^1.61.0",
    "tailwind-merge": "^3.0.1",
    "tailwind-variants": "^0.3.1",
    "usehooks-ts": "^3.1.1",
    "zod": "^3.24.1",
    "zod-formik-adapter": "^1.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@zenstackhq/tanstack-query": "2.11.6",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "postcss": "^8",
    "prettier-plugin-classnames": "^0.7.6",
    "prettier-plugin-merge": "^0.7.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "6.3.x",
    "tailwindcss": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "zenstack": "2.11.6"
  }
}
